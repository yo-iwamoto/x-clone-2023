{
  "compilerOptions": {
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "incremental": false,
    "resolveJsonModule": true
  }
}
